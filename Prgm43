import sqlite3 
con=sqlite3.connect('database2.sqlite3') 
cur=con.cursor() 
cur.execute('select max(salary)from employee') 
max=cur.fetchall() 
cur.execute('select min(salary)from employee') 
min=cur.fetchall() 
cur.execute('select sum(salary)from employee') 
sum=cur.fetchall() 
cur.execute('select avg(salary)from employee') 
avg=cur.fetchall() 
print('max sal is:',max) 
print('min sal is:',min) 
print('sum of sal is:',sum) 
print('avg sal is:',avg)
