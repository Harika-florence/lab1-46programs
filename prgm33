my_dict= {'1':['a', 'b'], '2':['c', 'd']}
my_list= list(my_dict.values())
for i in my_list[0]:
    for j in my_list[1]:
        print(i+j)
        #to get the maximum in the dictinoary 
my_list={'item1':45,'item2':55,'item3':75,'item4':90}
max_value = max(my_list, key=my_list.get)
print("Maximum is ",max_value)
